<?xml version="1.0" encoding="UTF-8" ?>
<templates id="template" inherit="point_of_sale.template">
    <t t-inherit="point_of_sale.ClientDetailsEdit" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('client-details-left')]" position="inside">
                <div class="client-detail geonames-div">
                    <span class="label">Auto-completion</span>
                    <select t-ref="inputAutocomplete" class="detail client-address-autocomplete needsclick" t-on-change="_updateChanges" 
                            name="completion">
                    </select>
                </div>
        </xpath>
    </t>
</templates>
